<script type="text/javascript">

function performSearchDialog()
{
var performSearch = $( "#performSearch" );

performSearch.css("position","absolute");


if ( $(window).height() > 0 )
{
performSearch.css("top", (($(window).height() - performSearch.outerHeight()) / 2) + $(window).scrollTop() + "px");
performSearch.css("left", (($(window).width() - performSearch.outerWidth()) / 2) + $(window).scrollLeft() + "px");
}else{
performSearch.css("top", (($(document).height() - performSearch.outerHeight()) / 2) + $(document).scrollTop() + "px");
performSearch.css("left", (($(document).width() - performSearch.outerWidth()) / 2) + $(document).scrollLeft() + "px");
}
performSearch.show();




if ( $.browser.msie )
{

setTimeout( function () { $( '#prgImgContainer' ).html( "<IMG SRC='/img/circleecm_progmeter40x40.gif' ALT='' BORDER='0'>" ); }, 300 );
}
}

</script>

