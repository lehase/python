<SCRIPT TYPE="text/javascript">
function Changefulltextslices( selectedIndex, valueStr ) {

if ( selectedIndex == 2 ) { 
clearSearchPrompt( 'fulltext' ); 
SearchBar.setFromHereQuery( 'document.fulltextSearchBarForm', 'fulltext', true ); 
document.fulltextSearchBarForm.locationId.value = '2000'; 
document.fulltextSearchBarForm.withinQuery.value = 'YES'; 
document.fulltextSearchBarForm.FromHereExpanded.value = 'TRUE'; 
document.fulltextSearchBarForm.submitButton.disabled = true; 
document.fulltextSearchBarForm.submitButton.name = 'TRUE'; 
document.fulltextSearchBarForm.objAction.value = 'searchPrompt'; 
document.fulltextSearchBarForm.slice.value = '52122758'; 
document.fulltextSearchBarForm.func.value = 'll'; 
document.fulltextSearchBarForm.submit() 
} 
if ( selectedIndex == 3 ) { 
selectedIndex = 1 
document.fulltextSearchBarForm.ScopeSelection.value = '52122758%7C2000%7CWithin%20%D0%A0%D0%B0%D0%B1%D0%BE%D1%87%D0%B0%D1%8F%20%D0%BA%D0%BE%D1%80%D0%BF%D0%BE%D1%80%D0%B0%D1%82%D0%B8%D0%B2%D0%BD%D0%B0%D1%8F%20%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D1%8C';  
} 

selectedIndex = parseInt(selectedIndex, 10);
document.fulltextSearchBarForm.ScopeSelection.value = valueStr;


document.getElementById('selectedfulltextslices').innerHTML = document.getElementById('fulltextslices' + selectedIndex + 'DivId').innerHTML;


generateSearchPromptIfNecessary();


document.getElementById('showfulltextslices').style.visibility = "hidden";

}

sd_setBaseURL( "/OTCS/cs.exe" );
sd_setSupportPath( "/img/" );
sd_setMenuName( "fulltextslices" );
sd_menuTxtStart( "Slices", "Select Search Type" , "From Here", "", "" );
