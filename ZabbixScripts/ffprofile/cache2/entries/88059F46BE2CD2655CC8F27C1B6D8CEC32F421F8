<SCRIPT type="text/javascript">

function ChangefulltextObjectType( selectedIndex, valueStr, menuName ) {


document.fulltextSearchBarForm.typeDropDownId.value = selectedIndex;
document.fulltextSearchBarForm.where2.value = valueStr;


if( document.getElementById( menuName + selectedIndex + 'DivId') != null )
{
document.getElementById('selected' + menuName ).innerHTML = document.getElementById( menuName + selectedIndex + 'DivId').innerHTML
}


if( document.getElementById('show' + menuName) != null )
{
document.getElementById('show' + menuName).style.visibility = "hidden"
}


}

document.fulltextSearchBarForm.typeDropDownId.value = '1';
document.fulltextSearchBarForm.where2.value = '';

sd_setupMenu( "/OTCS/cs.exe" , "/img/"  )

sd_setFormName ( "SearchBarForm" )
sd_setMenuName( "fulltextObjectType" )
sd_menuImgStart( "Object type", "Select Searchable Type" , "Everything", "itemID", "websbroker/star.gif" )
sd_optionsStart()

sd_itemImg( 'Everything', 'websbroker/star.gif', '1', '', 0 )
sd_separator( "2" )
sd_itemImg( 'All Documents', 'webdoc/doc.gif', '3', 'OTFilterMIMEType: qlrange &quot;vnd.ms-excel|msword|pdf|powerpoint|text|zip&quot;', 0 )
sd_itemImg( 'Excel', 'webdoc/appexel.gif', '4', 'OTFilterMIMEType:vnd.ms-excel', 1 )
sd_itemImg( 'Word', 'webdoc/appword.gif', '5', 'OTFilterMIMEType:msword', 1 )
sd_itemImg( 'PDF', 'webdoc/apppdf.gif', '6', 'OTFilterMIMEType:pdf', 1 )
