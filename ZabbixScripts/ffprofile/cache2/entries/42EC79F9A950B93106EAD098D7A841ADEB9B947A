<SCRIPT type="text/javascript">

function ChangefulltextDateType( selectedIndex, valueStr, menuName ) {


document.fulltextSearchBarForm.dateDropDownId.value = selectedIndex;
document.fulltextSearchBarForm.OTModifyDate1.value = valueStr;


if( document.getElementById( menuName + selectedIndex + 'DivId') != null )
{
document.getElementById('selected' + menuName ).innerHTML = document.getElementById( menuName + selectedIndex + 'DivId').innerHTML
}


if( document.getElementById('show' + menuName) != null )
{
document.getElementById('show' + menuName).style.visibility = "hidden"
}


}

document.fulltextSearchBarForm.dateDropDownId.value = '1';
document.fulltextSearchBarForm.OTModifyDate1.value = '';

sd_setupMenu( "/otcs/cs.exe" , "/img/"  )

sd_setFormName ( "SearchBarForm" )
sd_setMenuName( "fulltextDateType" )
sd_menuImgStart( "Last modified", "Select Modification Time Period" , "Anytime", "itemID", "" )
sd_optionsStart()

sd_itemImg( 'Anytime', '', '1', '', 0 )
sd_separator( "2" )
sd_itemImg( 'Modified in the past day', '', '3', 'pastday', 0 )
sd_itemImg( 'Modified in the past week', '', '4', 'pastweek', 0 )
