	$(function(){
		$(".filter_button")
			.button()
			.click(function(event){
				event.preventDefault();
			});
		
		$( "#dialog-message" ).dialog({
			modal: true,
			autoOpen: false,
			buttons: {
				Ok: function() {
					$(this).dialog( "close" );
				}
			}
		});
		
		$('#rename-search').html('<br><table><tr><td><b>Имя поиска:</b>&nbsp;&nbsp;</td><td><input type="text" size="20" /></td></tr></table>');
		$('#rename-search').dialog({
			modal: true,
			autoOpen: false,
			buttons: {
				Ok: function() {
					var old_name = $("#Searches option:selected").text();
					var new_name = $(this).find('input[type="text"]').val();
					var rename = true;
					$('#Searches').children().each(function(){
						if(!$(this).attr('selected')){
							if(new_name == $(this).val()){ rename = false; }
						}
					});
					$(this).dialog("close");
					$(this).find('input[type="text"]').val('');
					if(rename){
						if(new_name == ''){
							msgBox('Пожалуйста, заполните имя поиска.');
						} else {
							doRename(old_name, new_name);
						}
					} else { msgBox('Поиск с таким именем уже существует.'); }
				}
			}
		});
			
		$("#tabs").tabs({
			collapsible: true,
			active: 0,
			activate: function( event, ui ) {
				$( "#TAB" ).val(ui.newTab.index());
			}
		});
		
		$('.pageTitleText').replaceWith('<h1>Отчет по задачам</h1>');
		$('.pageTitleIcon').remove();
		

		var obj_onload;
		$.ajax({
            type: "POST",
            url: Common.zGlobalUrl != "" ? Common.zGlobalUrl : document.location.origin + document.location.pathname,
			dataType: "json",
			data:{
				func: 'nickname.do',
				nickname: "51653307",
				action: "open",
				SearchName: 'DEFAULT'
			},
			async: false,
			cache: false,
			success: function(response){
				if(response.actualRows > 0){
					$('#SOURCE_PARAM[value="' + response.myRows[0].SOURCETYPE + '"]').attr('checked', true);
					obj_onload = new Object({
						BARCODE: response.myRows[0].BARCODE,
						BUSAREA: response.myRows[0].BUSAREA,
						ORGANIZATIONDESC: response.myRows[0].ORGANIZATIONDESC,
						ORIG_COPY: response.myRows[0].ORIG_COPY,
						SCANSTATION: response.myRows[0].SCANSTATION,
						FRONTOFFICE: response.myRows[0].FRONTOFFICE,
						SCANUSER: response.myRows[0].SCANUSER,
						SCANDATE: [response.myRows[0].SCANDATEFROM == null ? '': response.myRows[0].SCANDATEFROM, response.myRows[0].SCANDATETO == null ? '': response.myRows[0].SCANDATETO],
						COURIER: response.myRows[0].COURIER,
						SSCARRIVAL: [response.myRows[0].SSCARRIVALFROM == null ? '': response.myRows[0].SSCARRIVALFROM,response.myRows[0].SSCARRIVALTO == null ? '': response.myRows[0].SSCARRIVALTO],
						SSCACCEPTANCE: [response.myRows[0].SSCACCEPTANCEFROM == null ? '': response.myRows[0].SSCACCEPTANCEFROM,response.myRows[0].SSCACCEPTANCETO == null ? '': response.myRows[0].SSCACCEPTANCETO],
						SSCDEPARTURE: [response.myRows[0].SSCDEPARTUREFROM == null ? '': response.myRows[0].SSCDEPARTUREFROM,response.myRows[0].SSCDEPARTURETO == null ? '': response.myRows[0].SSCDEPARTURETO],
						SSCUSER: response.myRows[0].SSCUSER,
						SSCDURATIONALL: [response.myRows[0].SSCDURATIONALLFROM == null ? '': response.myRows[0].SSCDURATIONALLFROM,response.myRows[0].SSCDURATIONALLTO == null ? '': response.myRows[0].SSCDURATIONALLTO],
						SSCCOMMENT: response.myRows[0].SSCCOMMENT,
						REJECTEDSCANUSER: response.myRows[0].REJECTEDSCANUSER,
						REJECTEDSCANDURATION: [response.myRows[0].REJECTEDSCANDURATIONFROM == null ? '': response.myRows[0].REJECTEDSCANDURATIONFROM, response.myRows[0].REJECTEDSCANDURATIONTO == null ? '': response.myRows[0].REJECTEDSCANDURATIONTO],
						REJECTEDSCANDEPARTURE: [response.myRows[0].REJECTEDSCANDEPARTUREFROM == null ? '': response.myRows[0].REJECTEDSCANDEPARTUREFROM, response.myRows[0].REJECTEDSCANDEPARTURETO == null ? '': response.myRows[0].REJECTEDSCANDEPARTURETO],
						STATUS: response.myRows[0].STATUS,
						ACTUALDOCUMENT: response.myRows[0].ACTUALDOCUMENT,
						WITHOUTERP: response.myRows[0].WITHOUTERP,
						HRARRIVAL: [response.myRows[0].HRARRIVALFROM == null ? '': response.myRows[0].HRARRIVALFROM,response.myRows[0].HRARRIVALTO == null ? '': response.myRows[0].HRARRIVALTO],
						HRACCEPTANCE: [response.myRows[0].HRACCEPTANCEFROM == null ? '': response.myRows[0].HRACCEPTANCEFROM,response.myRows[0].HRACCEPTANCETO == null ? '': response.myRows[0].HRACCEPTANCETO],
						HRDEPARTURE: [response.myRows[0].HRDEPARTUREFROM == null ? '': response.myRows[0].HRDEPARTUREFROM,response.myRows[0].HRDEPARTURETO == null ? '': response.myRows[0].HRDEPARTURETO],
						HRUSER: response.myRows[0].HRUSER,
						HRDURATIONALL: [response.myRows[0].HRDURATIONALLFROM == null ? '': response.myRows[0].HRDURATIONALLFROM,response.myRows[0].HRDURATIONALLTO == null ? '': response.myRows[0].HRDURATIONALLTO],
