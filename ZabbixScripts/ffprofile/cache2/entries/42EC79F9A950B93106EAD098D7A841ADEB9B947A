						SELECTED: response.myRows[0].SELECTED,
                        SCANPROFILE: response.myRows[0].SCANPROFILE,
                        DOCTYPEDESC: response.myRows[0].DOCTYPEDESC
					});	
				}
			}
		});
		
		MapDataToView(obj_onload);
		if(("0" == 1) && ('START' == 'START')) { doSearch(false); }
		
		$('#Add_Where').change(function(){
			if($(this).val() != ''){ AddWhereItem($(this).find('option:selected').val(), undefined); }
		});
		
		$("#Add_Order").change(function(){
			if($(this).val() != ""){ AddOrderItem($(this).find("option:selected").val(), undefined); }
		});
		
		$(".connected_sortable").sortable({
			receive: function( event, ui ) {
				var _item = ui.item;
				if(_item.hasClass("state-grey")){
					_item.switchClass("state-grey","ui-state-default",0);
				} else {
					_item.switchClass("ui-state-default","state-grey",0);
				}
				if($("#available_fields").children().length == 0){
					$("#select_all").attr("checked",true);
				} else {
					$("#select_all").attr("checked",false);
				}
			},
			connectWith: ".connected_sortable"
		}).disableSelection();
		
		$("#select_all").click(function(){
			if(this.checked){
				if($("#available_fields").children().length > 0){
					$("#available_fields").children().each(function(){
						$(this).switchClass("state-grey","ui-state-default",0);
						var current = $(this);
						$("#selected_fields").append($(this));
					});
				}
			} else {
				if($("#selected_fields").children().length > 0){
					$("#selected_fields").children().each(function(){
						$(this).switchClass("ui-state-default","state-grey",0);
						$("#available_fields").append($(this));
					});
				}
			}
		});
		
		$.ajax({
            type: "POST",
            url: Common.zGlobalUrl != "" ? Common.zGlobalUrl : document.location.origin + document.location.pathname,
			dataType: "json",
			data:{
				func: 'nickname.do',
				nickname: "51655062",
				action: "open"
			},
			async: false,
			cache: false,
			success: function(response){
				if(response.actualRows > 0){
					for(var j = 0;j < response.actualRows; j++){
						$('#Searches').append('<option public="' + response.myRows[j].PUBLIC + '" >' + response.myRows[j].SEARCH + '</option>');
					}
				}
			}
		});
		
		if("" != ""){
			$('#Searches').children().each(function(){
				if($(this).text() == ""){
					$(this).attr('selected', 'selected');
				}
			});
		}

		if(CheckUserPermissions()){
			$('#SAVED_SEARCHES').find('input[type="checkbox"]').attr('disabled', false);
		}
		
		$('#tabs,#search_panel,#BrowseViewFrm,#BrowsePageRowPagingSectionCellTable').show();
	});
