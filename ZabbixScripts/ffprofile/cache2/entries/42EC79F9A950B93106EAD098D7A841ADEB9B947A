function Changefulltextslices( selectedIndex, valueStr ) {

selectedIndex = parseInt(selectedIndex, 10);
document.fulltextSearchBarForm.ScopeSelection.value = valueStr;


document.getElementById('selectedfulltextslices').innerHTML = document.getElementById('fulltextslices' + selectedIndex + 'DivId').innerHTML;


generateSearchPromptIfNecessary();


document.getElementById('showfulltextslices').style.visibility = "hidden";

}

